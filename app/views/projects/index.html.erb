<div class="container">
  <div class="row" id="projects-row">
    <% @projects.each do |project| %>
      <%= render project %>
    <% end %>
  </div>
</div>


<div class="form-container">
  <div class="col-xs-12 col-sm-4" id="new-project-button-div">
    <% if user_signed_in? %>
      <div class="card-2">
        <%= link_to 'add new project', new_project_path, id: "new-project-button", remote: true %>
      </div>
    <% end %>
  </div>
</div>
